{
  "executionRoleArn": "arn:aws:iam::<AWS_ACCOUNT_ID>:role/<APP_NAME>_ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "demo-service-new",
      "image": "demo-service-new",
      "essential": true,
      "memoryReservation":100,
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "PORT",
          "value": "8080"
        },
        {
          "name": "APP_NAME",
          "value": "demo-service-new"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": null,
  "family": "demo-service-new"
}
